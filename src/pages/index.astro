---
import MainLayout from "../layouts/MainLayout.astro";
import OptionPlain from "../components/Options/OptionPlain.astro";
import Restriction from "../scripts/restriction";
import Escalation from "../components/Escalation.astro";

const title = "Home Page";

const sa1 = new Restriction();
const sa2 = new Restriction({ code: "sa56" });
---

<MainLayout title={title}>
	<Escalation />
	<OptionPlain restriction={sa1} />
	<OptionPlain restriction={sa2} />
</MainLayout>
